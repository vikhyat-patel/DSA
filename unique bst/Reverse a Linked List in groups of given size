class Solution
{
    public:
    struct node *reverse (struct node *head, int k)
    { 
       node* curr = head;
       node* prev = NULL;
       node* temp;
       int count=0;
       while(curr!=NULL&&count<k){
           temp = curr->next;
           curr->next = prev;
           prev = curr;
           curr = temp;
           count++;
       }
       if(curr!=NULL)
       head->next = reverse(curr,k);
       return prev;
    }
};
